<div id="content_sub">
	<div id="content_spacer">
		<h2>New photo</h2>

<%= error_messages_for :photo %>

<% form_for(@photo, :url => admin_product_photos_path, :html => {:multipart => true}) do |f| %>
  <p><%= f.file_field :uploaded_data %></p>
  <p><%= f.submit "Upload Photo", :class => :crd %></p>
<% end %>

<p><%= link_to 'Back', admin_product_photos_path(@product) %></p>
</div></div>